mixin read-more(props = {})
  - var link = props.link
  - var external = props.external
  - var title = props.title

  a(href=link target=external && "_blank" class="inline-flex items-center link link--underline")
    span(class="")
      | !{title}
    span(class="ml-5 w-[0.8em] h-[0.8em] inline-block align-baseline")
      | !{filters.icon('plus')}

mixin icon(icon, size = 'large')
  -
    var sizes = {
      large: 'custom-icon--large',
      small: 'custom-icon--small',
    }
  div(class="custom-icon" class=sizes[size])
    img(src=icon class="w-full h-full object-contain" alt="icon")

mixin lamp-icon()
  div(class="custom-icon custom-icon--large !p-10 laptop:!p-14")
    +custom-icon({type: 'lamp', attr: {class: 'relative -bottom-10 laptop:-bottom-14'}})

mixin badge(text, attr = {})
  //- - var text = props.text
  //- - var attr = props.attr
  button(type="button" class="inline-flex items-center text-center py-10 px-20 backdrop-blur text-cream border-2 border-cream border-opacity-10 hover:opacity-70 transition-opacity duration-300")&attributes(attr)
    | !{text}



mixin default-accordion(props = {})
  - var attr = props.attr
  - var items = props.items

  div(class="relative" data-component="accordion")&attributes(attr)
    each item, index in items

      div(data-accordion-item='' class="relative")
        div(class="flex items-center justify-between py-10 text-base leading-base" data-accordion-opener='')

        div(data-accordion-content='')

mixin tooltip(props = {})
  span(class="ml-5 -mt-2 laptop:ml-8 w-12 h-12 laptop:w-20 laptop:h-20 inline-block cursor-pointer" data-cursor=props.text)
    | !{filters.svg('info', 'w-full h-full object-cover')}


mixin preorder-form(props = {}, colorOptions, hideImage = false)
  - var title = props.title
  - var description = props.description
  - var deliveryTooltip = props.deliveryTooltip
  - var purchaseTooltip = props.purchaseTooltip
  - var fullPrice = props.fullPrice
  - var currentPrice = props.currentPrice
  - var image = props.image.data
  - var redirectTitle = props.redirectTitle || 'Redirecting to Checkout'
  - var redirectDescription = props.redirectDescription || 'You’re automatically being redirected to finalize your purchase.'
  - var manualRedirect = props.manualRedirect

  div(class="bg-cream text-gray-900 rounded p-12 md:p-10 relative grid gap-10" class=!hideImage && 'md:grid-cols-2' class=hideImage ? 'max-w-[45.6rem]' : 'max-w-[93rem]')
    if !hideImage && image.attributes
      div(class="rounded aspect-[276/230] md:aspect-none overflow-hidden md:w-auto md:h-full")
        +picture({
          ...image.attributes,
          pictureClass: 'h-full',
          imageClass: 'object-cover'
        })

    form(data-component="purchase-form" class="relative" novalidate='' )
      div(data-layout="redirect" class='absolute left-0 top-0 w-full h-full opacity-0 pointer-events-none py-10 md:p-10 bg-cream z-2')
        div(class="relative h-full flex flex-col")
          span(data-cancel-form='' class="absolute right-0 top-0 w-16 h-16 hover:opacity-50 transition-opacity duration-300 cursor-pointer")
            | !{filters.svg('close', 'w-full h-full')}

          div(class="mb-10 md:mb-30")
            +lamp-icon()

          h4(class="mb-15" data-dots="....")
            | !{redirectTitle}

          p(class="mb-25")
            | !{redirectDescription}

          if manualRedirect
            div(class="mt-auto")
              a(href=manualRedirect.link class="link link--underline")
                | !{manualRedirect.title}



      div(data-layout="purchase" class="py-10 md:p-10")
        div(class="mb-10 md:mb-30")
          +lamp-icon()

        h4(class="mb-15")
          | !{title}

        p(class="reset-last flex items-center mb-12")
          | !{description}
          +tooltip({text: deliveryTooltip})

        div(class="border-t border-b border-gray-900 border-opacity-20 py-12 flex items-center gap-x-20")
          div
            | ATMOS price: !{fullPrice}
          div(class="font-semibold flex items-center")
            | Due Today: !{currentPrice}
            +tooltip({text: purchaseTooltip})

        div(class="grid gap-y-10 laptop:block")
          div(class="pt-20 laptop:pt-0 laptop:mb-20 relative" data-component="color-picker")
            div(class="laptop:hidden h-40 rounded-full bg-gray-900 bg-opacity-[0.06] py-10 px-20 flex items-center justify-between relative border border-transparent" data-opener='')
              - var firstColorOption = colorOptions[0]
              div(data-text='' class="font-medium")
                div(class="flex items-center absolute left-0 top-0 h-full w-full p-6")
                  span(class="w-[2.8rem] h-[2.8rem] laptop:w-[3.2rem] laptop:h-[3.2rem] mr-8 rounded-full flex-shrink-0 overflow-hidden")
                    +picture({
                      ...firstColorOption.image.data.attributes,
                      imageClass: 'w-full h-full object-cover'
                    })

                  span(class="font-medium text-xs laptop:text-sm laptop:max-w-60")
                    | !{firstColorOption.title}

              span(class="w-8 h-8 flex items-center justify-end")
                <svg width="6" height="5" viewBox="0 0 6 5" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M3.86603 4.5C3.48112 5.16667 2.51887 5.16667 2.13397 4.5L0.401924 1.5C0.0170235 0.833332 0.498149 -5.6841e-07 1.26795 -5.01112e-07L4.73205 -1.9827e-07C5.50185 -1.30972e-07 5.98298 0.833333 5.59808 1.5L3.86603 4.5Z" fill="currentColor"/>
                </svg>

            div(class="bg-gray-100 absolute left-0 top-full w-full mt-4 laptop:static laptop:mt-0 laptop:bg-transparent rounded laptop:rounded-none laptop:border-b laptop:border-gray-900 laptop:border-opacity-20 laptop:py-14 laptop:flex laptop:items-center laptop:gap-x-30 z-2" data-colors='')
              each item, index in colorOptions
                - var first = index === 0
                - var slug = filters.slugify(item.title)
                - var image = item.image.data

                label(class="group flex items-center py-12 px-6 laptop:p-0 cursor-pointer" class=!first && 'border-t border-gray-900 border-opacity-20 laptop:border-0' for=slug data-color=slug)
                  input(type="radio" required='' class="sr-only" name="color_option" value=slug id=slug checked=first data-image-url=filters.getUploadsPath(image.attributes.url) data-color-name=item.title)
                  if image
                    span(class="w-[2.8rem] h-[2.8rem] laptop:w-[3.2rem] laptop:h-[3.2rem] mr-8 rounded-full flex-shrink-0 overflow-hidden")
                      +picture({
                        ...image.attributes,
                        imageClass: 'w-full h-full object-cover'
                      })

                    span(class="font-medium text-xs laptop:text-sm laptop:max-w-60")
                      | !{item.title}

            div(data-error-field='')

          div(class="laptop:mb-10")
            +form-control({
              type: 'email',
              attr: {
                required: true,
                name: 'email',
                placeholder: 'Email',
              }
            })
          div(class="laptop:mb-10")
            +button({
              type: 'submit',
              text: 'Pre-Order Now',
              attr: {
                class: 'w-full'
              }
            })

          div(class="pt-3 opacity-40")
            | By clicking "Pre-Order Now" you agree to the &nbsp;
            a(class="link link--underline" target="_blank" href=cms.globalInfo.termsLink) terms
            | &nbsp;and&nbsp;
