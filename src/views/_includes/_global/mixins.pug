

mixin button(props = {})
  -
    var theme = props.theme || 'primary'
    var size = props.size
    var attr = props.attr
    var type = props.type
    var iconAttr = props.iconAttr
    var text = props.text
    var active = props.active
    var external = props.external
    var icon = props.icon
    var svg = props.svg || 'arrow-right'
    var iconPrepend = props.iconPrepend || false
    var tag = attr && attr.href ? 'a' : 'button';
    var animationType = props.animationType || 'btn-icon-movement--right'
    var themes = {
      primary: 'btn--primary',
      secondary: 'btn--secondary',
    }

  #{tag}(type=(tag == 'button') ? type || 'button': false class=`btn` target=external && '_blank' class=themes[theme] class=active ? 'is-active' : false)&attributes(attr)
    | !{text}
    if block
      block



mixin picture(props = {}, lazy = true)
  - var url = props.local ? props.url : filters.getUploadsPath(props.url)
  - var alt = props.alternativeText || 'image description'
  - var attr = props.attr
  - var width = props.width
  - var height = props.height
  - var disableResponsive = props.disableResponsive
  - var hideBackground = props.hideBackground
  - var aspect = (width / height)
  - var formats = props.formats
  - var mobileUrl = props.mobileUrl || url
  - var baseImageClass = props.baseImageClass || 'w-full'

  picture(class=`w-full h-auto flex` style=aspect && `--aspect: ${aspect}` class=props.pictureClass)&attributes(attr)
    if !disableResponsive
      source(
        media=`(min-width: 1000px)`
        data-srcset=lazy && url
        srcset=!lazy && url)

    img(
      class=`block`
      class=baseImageClass
      class=props.imageClass
      class=lazy ? 'opacity-0' : false
      data-component=lazy ? "lazyload" : false
      data-src=lazy ? disableResponsive ? url : mobileUrl : false
      src=lazy ? false : disableResponsive ? url : mobileUrl
      alt=alt || 'image description'
      width=width
      height=height
      draggable="false"
    )

mixin parallax-media(props = {})
  -
    var baseClass = props.baseClass
    var parallaxAttr = props.parallaxAttr
    var speed = props.speed || 3
    var attr = Object.assign({}, {
      class: 'h-full overflow-hidden',
    }, props.attr || {})

    var parallax = Object.assign({}, {
      'data-animation': 'parallax',
      'data-scroll-speed': speed,
      'data-start': 'top bottom+=100',
      'data-end': 'bottom top-=100'
    }, props.parallax || {})

  div(class="relative w-full overflow-hidden" class=baseClass)&attributes(attr)
    div(class="h-full")&attributes(parallax)&attributes(parallaxAttr)
      if block
        block

mixin wys(props = {})
  - var attr = props.attr
  - var bodyCopy = props.bodyCopy
  - var resetLast = props.resetLast !== undefined ? props.resetLast : true
  - var animation = props.animation

  div(class="wysiwyg" class=resetLast && 'reset-last')&attributes(attr, animation)
    if bodyCopy
      | !{bodyCopy}

mixin form-control(props, secondaryStyle)
  - var elType = props.type
  - var holderAttr = props.holderAttr

  div(class="form-control")
    if elType === 'checkbox'
      label(class="flex items-baseline")
        input(type="checkbox")&attributes(props.attr)
        if block
          block
        else
          | !{props.label}
    else if elType === 'select'
      - var options = props.options
      div(class="group relative input-group")&attributes(holderAttr)
        select(class="js-custom-select")&attributes(props.attr)
          option(value="" disabled selected)=props.placeholder
          each item in options
            option(value=item.code data-id=item.id)=item.name
    else
      div(class="group relative input-group")&attributes(holderAttr)
        if elType === 'textarea'
          textarea(class="form-control" class=secondaryStyle && 'form-control--secondary' autocomplete="off")&attributes(props.attr)
        else
          input(type=elType class="form-control" class=secondaryStyle && 'form-control--secondary' autocomplete="off")&attributes(props.attr)

        div(data-error-field=props.attr.name || '')=props.error

